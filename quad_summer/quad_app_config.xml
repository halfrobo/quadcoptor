<?xml version="1.0" encoding="UTF-8"?>
<root>
    <tab name="Graphs" num_cols="1" num_rows="1">
        <graph name="All params" start_col="0" start_row="0" num_cols="1" num_rows="1">
            <series name="yaw" color="ffff0000" prefix="y"></series>
            <series name="pitch" color="ff00ff00" prefix="p"></series>
            <series name="roll" color="ff0000ff" prefix="r"></series>

            <series name="gyro yaw" color="ff000000" prefix="gy"></series>
            <series name="gyro pitch" color="ff00FFFF" prefix="gp"></series>
            <series name="gyro roll" color="ff008000" prefix="gr"></series>

            <series name="Pid yaw" color="ffDEB887" prefix="sy"></series>
            <series name="Pid pitch" color="ff7FFF00" prefix="sp"></series>
            <series name="Pid roll" color="ffFF7F50" prefix="sr"></series>

            <series name="base speed" color="ff00008B" prefix="bs"></series>
            <series name="m1 speed" color="ffFF1493" prefix="m1"></series>
            <series name="m2 speed" color="ffFFD700" prefix="m2"></series>
        </graph>
    </tab>
    <tab name="PID constants" num_cols="6" num_rows="1">
        <slider start_col="0" start_row="0" num_rows="1" num_cols="1" param_name="kp" command="kp=%;"></slider>
        <slider start_col="1" start_row="0" num_rows="1" num_cols="1" param_name="kd" command="kd=%;"></slider>
        <slider start_col="2" start_row="0" num_rows="1" num_cols="1" param_name="ki" command="ki=%;"></slider>
        <slider start_col="3" start_row="0" num_rows="1" num_cols="1" param_name="ykp" command="ykp=%;"></slider>
        <slider start_col="4" start_row="0" num_rows="1" num_cols="1" param_name="ykd" command="ykd=%;"></slider>
        <slider start_col="5" start_row="0" num_rows="1" num_cols="1" param_name="yki" command="yki=%;"></slider>
    </tab>
    <tab name="PID bounds" num_cols="7" num_rows="1">
        <slider start_col="0" start_row="0" num_rows="1" num_cols="1" param_name="angle_pr_bound" command="prb=%;"></slider>
        <slider start_col="1" start_row="0" num_rows="1" num_cols="1" param_name="rate_pr_bound" command="gprb=%;"></slider>
        <slider start_col="2" start_row="0" num_rows="1" num_cols="1" param_name="i_pr_bound" command="iprb=%;"></slider>
        <slider start_col="3" start_row="0" num_rows="1" num_cols="1" param_name="angle_y_bound" command="yprb=%;"></slider>
        <slider start_col="4" start_row="0" num_rows="1" num_cols="1" param_name="rate_y_bound" command="ygprb=%;"></slider>
        <slider start_col="5" start_row="0" num_rows="1" num_cols="1" param_name="i_y_bound" command="yiprb=%;"></slider>
        <slider start_col="6" start_row="0" num_rows="1" num_cols="1" param_name="i_milli" command="miterm=%;"></slider>
    </tab>
    <tab name="RF effects and PId full bound" num_cols="7" num_rows="1">
        <slider start_col="0" start_row="0" num_rows="1" num_cols="1" param_name="pitch_effect" command="pe=%;"></slider>
        <slider start_col="1" start_row="0" num_rows="1" num_cols="1" param_name="roll_effect" command="re=%;"></slider>
        <slider start_col="2" start_row="0" num_rows="1" num_cols="1" param_name="yaw_effect" command="ye=%;"></slider>
        <slider start_col="3" start_row="0" num_rows="1" num_cols="1" param_name="throttle_min" command="tl=%;"></slider>
        <slider start_col="4" start_row="0" num_rows="1" num_cols="1" param_name="throttle_max" command="th=%;"></slider>
        <slider start_col="5" start_row="0" num_rows="1" num_cols="1" param_name="pid_pr_bound" command="sprb=%;"></slider>
        <slider start_col="6" start_row="0" num_rows="1" num_cols="1" param_name="pid_y_bound" command="syb=%;"></slider>
    </tab>

</root>
